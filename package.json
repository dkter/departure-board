    "resources": {
      "media": [
        {
          "type": "raw",
          "name": "STREETCAR_ANIM",
          "file": "streetcar_frames.pdc"
        },
        {
          "type": "raw",
          "name": "SUBWAY_ANIM",
          "file": "srt_frames.pdc"
        },
        {
          "type": "raw",
          "name": "BUS_ANIM",
          "file": "bus_frames.pdc"
        },
        {
          "type": "raw",
          "name": "TRAIN_ANIM",
          "file": "train_frames.pdc"
        }
      ]
    }
