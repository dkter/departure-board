{
  "name": "bumblebee",
  "author": "MakeAwesomeHappen",
  "version": "1.0.0",
  "keywords": ["pebble-app"],
  "private": true,
  "dependencies": {},
  "capabilities": [ "location" ],
  "pebble": {
    "displayName": "bumblebee",
    "uuid": "a18a191e-51e0-48a3-9c63-075eea857333",
